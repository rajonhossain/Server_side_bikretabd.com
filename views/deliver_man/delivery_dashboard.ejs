	<style>
		.sidebar_inactive {
			overflow-y: hidden;
			filter: grayscale(50%) blur(5px);
			pointer-events: none;
			position: fixed;
		}

		.content_inactive {
			overflow-y: hidden;
			filter: grayscale(50%) blur(5px);
			pointer-events: none;

		}
